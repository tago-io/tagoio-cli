{
  "name": "@tago-io/cli",
  "version": "2.0.0",
  "description": "TagoIO Application CLI Node.JS",
  "main": "./build/index.js",
  "repository": "tago-io/tagoio-cli",
  "keywords": [
    "tago",
    "tagoio",
    "tago-io",
    "iot",
    "tago.io",
    "cli",
    "analysis",
    "device"
  ],
  "engines": {
    "node": ">=17.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "start": "node -r @swc-node/register ./src/index.ts",
    "build": "rm -rf ./build; tsc --build; chmod +x ./build/index.js",
    "test": "vitest",
    "test:single": "vitest --",
    "linter": "biome lint ./src --no-errors-on-unmatched --diagnostic-level=error",
    "linter-fix": "biome lint --apply ./src"
  },
  "bin": {
    "tagoio": "./build/index.js"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "build"
    ],
    "preset": "ts-jest"
  },
  "author": "TagoIO LLC",
  "license": "ISC",
  "dependencies": {
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.14",
    "@tago-io/sdk": "^11.3.8",
    "async": "^3.2.6",
    "axios": "^1.7.9",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "envfile": "^7.1.0",
    "kleur": "^4.1.5",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "prompts": "^2.4.2",
    "string-comparison": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/async": "^3.2.24",
    "@types/lodash": "^4.17.15",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.1",
    "@types/prompts": "^2.4.9",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.5"
  }
}
