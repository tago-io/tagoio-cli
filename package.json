{
  "name": "@tago-io/cli",
  "version": "2.1.0",
  "description": "TagoIO Application CLI Node.JS",
  "main": "./build/index.js",
  "repository": "tago-io/tagoio-cli",
  "keywords": ["tago", "tagoio", "tago-io", "iot", "tago.io", "cli", "analysis", "device"],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=6.0.0"
  },
  "files": ["build"],
  "scripts": {
    "start": "node -r @swc-node/register ./src/index.ts",
    "build": "rm -rf ./build; tsc --build; chmod +x ./build/index.js",
    "test": "vitest",
    "test:single": "vitest --",
    "linter": "biome lint ./src --no-errors-on-unmatched --diagnostic-level=error",
    "linter-fix": "biome lint --apply ./src"
  },
  "bin": {
    "tagoio": "./build/index.js"
  },
  "jest": {
    "testPathIgnorePatterns": ["build"],
    "preset": "ts-jest"
  },
  "author": "TagoIO LLC",
  "license": "ISC",
  "dependencies": {
    "@swc-node/register": "^1.10.10",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@tago-io/sdk": "^12.0.3",
    "async": "^3.2.6",
    "axios": "^1.11.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "envfile": "^7.1.0",
    "eventsource": "^4.0.0",
    "kleur": "^4.1.5",
    "lodash": "^4.17.21",
    "luxon": "^3.7.1",
    "prompts": "^2.4.2",
    "string-comparison": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/async": "^3.2.25",
    "@types/eventsource": "^3.0.0",
    "@types/lodash": "^4.17.20",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.2.1",
    "@types/prompts": "^2.4.9",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.5",
    "vitest": "^3.2.4"
  }
}
